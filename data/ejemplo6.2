variables 
x1 in [0, 0.5]; 
x2 in [0, 0.5]; 
x3 in [0, 0.5]; 
x4 in [0, 0.5]; 
x5 in [0, 0.5];

minimize (6x1^2 + 7x2^2 - 8x3^2 + 9x4^2 + 10*x5^2);

constraints (sin(pi/3)sqrt(x1) + cos(pi/6)sqrt(x2)) >= sqrt(x3) -  fo_restriccion; 
(-x1)^2 + (-x2)^2 + x4^2 + (0.5x5)^2 <= 0.7(sin(pi/4)sqrt(x1) + cos(pi/6)sqrt(x2)); 
-3x1 - 2x2 + x3 + x4 + x5 = 0; 
end
