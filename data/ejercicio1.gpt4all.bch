

variables
x1 in [ 0, 100]; 
x2 in [0, 100]; 
x3 in [0, 100]; 
x4 in [0, 100]; 
x5 in [0, 100]; 

minimize - (x1^2 + 2*x2^2 + 3*x3^2 + 4*x4^2 + 5*x5^2); 

contraints
- 0.5*((x1 - 2)^2 + 2*(x2 - 2)^2 + 3*(x3 - 2)^2 + 4*(x4 - 2)^2 + 5*(x5 - 2)^2) <= 9;
2*x1 - x2 - x3 - 9*x4 + 3*x5 + 7*x6 + x8 + 2*x9 + 5*x10 - 7*x11 - 4*x12 - 6*x13 - 3*x14 + 7*x15 - 5*x16 + x17 + x18 + 2*x20 <= 9;
- x1 - x2 - 9*x3 + 3*x4 + 5*x5 + x8 + 7*x9 - 7*x10 - 4*x11 - 6*x12 - 3*x13 + 7*x14 - 5*x16 + x17 + x18 + 2*x20 <= 9;
x1 + x2 + x3 + x4 + x5 + x6 + x7 + x8 + x9 + x10 + x11 + x12 + x13 + x14 + x15 + x16 + x17 + x18 + x19 + x20 <= 40;
end 
